					<association-type id="#t-1281990" name="TestAssocTypeName11" weight-ahead="0.5" weight-aback="0.7" />
					<association-type id="#t-2836564" name="TestAssocTypeName12" weight-ahead="0.5" weight-aback="0.7" />
